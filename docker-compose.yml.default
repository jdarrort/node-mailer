#docker-compose --project-name=web-bank up -d
version: "3"
services:
  web-bank:
    image: rws-mailer
    ports:
     - "8089:8100"
    environment:
     - SMTP_SERVER=my.smtp.server
     - SMTP_USER_LOGIN=my.user
     - SMTP_USER_PWD=my.user.pwd
     - SMTP_USER_EMAIL=my.user.email
     - DEFAULT_RECIPIENT=def.recipient.email
networks:
  default:
    external:
      name: rws